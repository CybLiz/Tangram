.triangle {
    transform: translate(212px, 127px) rotate(180deg);
    transition: all 2s;
}

.triangle2 {
    transform: translate(151px, 335px) rotate(135deg);
    transition: all 2s;
}

.triangle3 {
    transform: translate(17px, 197px) rotate(270deg);
    transition: all 2s;
}

.triangle4 {
    transform: translate(55px, 130px) rotate(180deg);
    transition: all 2s;
}

.triangle5 {
    transform: translate(160px, 130px);
    transition: all 2s;
}

.carre {
    transform: translate(175px, 90px) rotate(45deg);
    transition: all 2s;
}

.parallelogram {
    clip-path: polygon(35% 0%, 95% 0%, 69% 100%, 9% 100%);
    transform: translate(172px, 58px) rotate(44deg);
    transition: all 2s;
    width: 100px;
    height: 50px;
}